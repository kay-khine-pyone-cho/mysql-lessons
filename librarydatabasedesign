CREATE TABLE IF NOT EXISTS librians(
id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
firstname VARCHAR(30) NOT NULL,
lastname VARCHAR(30) NOT NULL,
username VARCHAR(50) NOT NULL,
password VARCHAR(20) NOT NULL
);

CREATE TABLE librianinfos(
id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
librian_id INT NOT NULL,
phone VARCHAR(20),
email VARCHAR(20) NOT NULL UNIQUE,
dob DATE,
address VARCHAR(50) NOT NULL,
coverphoto VARCHAR(50),
profilephto VARCHAR(50),
created_at TIMESTAMP DEFAULT NOW(),
updated_at TIMESTAMP DEFAULT NOW()
FOREIGN KEY(librian_info) REFERENCE ON librian.id ON CASCADE DELETE ON CASCADE UPDATE
)